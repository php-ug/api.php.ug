{
    "name": "zendframework/zend-expressive-skeleton",
    "description": "Zend expressive skeleton. Begin developing PSR-7 middleware applications in seconds!",
    "type": "project",
    "homepage": "https://github.com/zendframework/zend-expressive-skeleton",
    "license": "BSD-3-Clause",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "bitexpert/disco": "^0.9.0",
        "dasprid/container-interop-doctrine": "^1.1",
        "geocoder-php/nominatim-provider": "^4.1",
        "http-interop/http-middleware": "^0.4",
        "org_heigl/trait-iterator": "^1.0",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.6",
        "psr/http-server-middleware": "^1.0",
        "roave/security-advisories": "dev-master",
        "willdurand/geocoder": "^4.2",
        "xtreamwayz/pimple-container-interop": "^1.0",
        "zendframework/zend-component-installer": "^1.0",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-expressive": "^2.0.5",
        "zendframework/zend-expressive-fastroute": "^2.0",
        "zendframework/zend-expressive-helpers": "^4.0",
        "zendframework/zend-expressive-twigrenderer": "^1.4",
        "zendframework/zend-permissions-acl": "^2.6",
        "zendframework/zend-stdlib": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.8 || ^5.7.15",
        "squizlabs/php_codesniffer": "^2.8.1",
        "zfcampus/zf-development-mode": "^3.1",
        "zendframework/zend-expressive-tooling": "^0.4.1",
        "filp/whoops": "^2.1.7"
    },
    "autoload": {
        "psr-4": {
            "Phpug\\Api\\": "src/Api/src/",
            "Phpug\\Generic\\": "src/Generic/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpug\\ApiTest\\": "test/ApiTest/",
            "Phpug\\GenericTest\\": "test/GenericTest/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "check": [
            "@cs-check",
            "@test"
        ],
        "clear-config-cache": "php bin/clear-config-cache.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v"
    }
}
